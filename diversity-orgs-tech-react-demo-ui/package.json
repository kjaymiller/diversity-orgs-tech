{
  "name": "app-search-reference-ui-react",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@elastic/react-search-ui": "1.4.1",
    "@elastic/search-ui-app-search-connector": "1.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "build:css": "NODE_ENV=production && npx tailwindcss-cli@latest build src/tailwind.css -o src/main.css",
    "watch:css": "NODE_ENV=development && npx tailwindcss-cli@latest build src/tailwind.css -o src/main.css",
    "start": "npm run watch:css && react-scripts start",
    "start-dev": "npm-run-all -p watch-css start",
    "build-js": "node ./scripts/build-no-chunks.js",
    "build": "npm run build:css && npm run build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^8.3.1",
    "rewire": "^4.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "chrome 44",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
